              "profiles": {
                "commonCaregiverProfile": {
                  "id": "29651746",
                  "bio": {
                    "experienceSummary": "I am a certified patient care technician/home health aide with more than 2 1/2 yrs of employment experience with excellent communications skills, providing exceptional in-home care and support to clients with a range of illnesses and other challenges.\nDuties and responsibilities include but not limited to light housekeeping, running errands and/or shopping, provide transportation to and from doctors appointments, assisting with light exercise, preparing meals when needed and giving medication reminders whether prescribed or OTC. Providing emotional support and companionship, assisting with personal grooming, providing palliative care and coordinating with family and medical staff when needed. Providing some assistance with pets.\nResume and references available upon request.",
                    "__typename": "Bio"
                  },
                  "__typename": "CommonCaregiverProfile"
                },
                "childCareCaregiverProfile": null,
                "petCareCaregiverProfile": null,
                "houseKeepingCaregiverProfile": null,
                "tutoringCaregiverProfile": null,
                "seniorCareCaregiverProfile": {
